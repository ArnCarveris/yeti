.PHONY: all test docs clean \
        yeti

all: yeti

test:
  # TODO: Add testing.

docs:
	@echo "[DOXYGEN] Doxyfile"
	@doxygen Doxyfile

clean:
	@echo "[RM] $(YETI_BUILD)/obj"
	@rm -R -f $(YETI_BUILD)/obj
	@echo "[RM] $(YETI_BUILD)/lib"
	@rm -R -f $(YETI_BUILD)/lib
	@echo "[RM] $(YETI_BUILD)/bin"
	@rm -R -f $(YETI_BUILD)/bin
	@echo "[RM] $(YETI_BUILD)/pkg"
	@rm -R -f $(YETI_BUILD)/pkg
