ifeq (@(BUTANE_BUILD),debug)
SUFFIX = _dbg
LIB_SUFFIX = -dbg
endif

ifeq (@(BUTANE_BUILD),development)
SUFFIX = _dev
endif

ifeq (@(TUP_ARCH),i386)
SUFFIX = $(SUFFIX)_x86
endif

ifeq (@(TUP_ARCH),x86_64)
SUFFIX = $(SUFFIX)_x86_64
endif

ifeq (@(TUP_ARCH),arm)
SUFFIX = $(SUFFIX)_arm
endif

LIB_SUFFIX = $(LIB_SUFFIX)-@(TUP_ARCH)
